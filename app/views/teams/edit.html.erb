<%= form_with url: "/teams/#{@team.id}", method: :patch, data: { turbo: false } do |form| %>
<%= form.hidden_field :division_id, :value => @team.division_id %>
<%= form.label :name %>
<%= form.text_field :name, :value => @team.name, :size => "25", :value => @team.name %>
<%= form.label :wins %>
<%= form.text_field :wins, :value => @team.wins, :size => "8" %>
<%= form.label :losses %>
<%= form.text_field :losses, :value => @team.losses, :size => "8" %>
<%= form.label :ties %>
<%= form.text_field :ties, :value => @team.ties, :size => "8" %>
<%= form.label :rank %>
<%= form.text_field :rank, :value => @team.rank, :size => "5" %>
<%= form.label :year_started %>
<%= form.text_field :year_started, :value => @team.year_started, :size => "5" %>
<%= form.label :won_super_bowl %>
<%= form.text_field :super_bowl, :value => @team.super_bowl, :size => "5" %>
<%= form.submit "Update", :class => "submit" %>
<% end %>